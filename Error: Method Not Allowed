68556 silly resolved        pretest: 'npm run --silent readme && npm run --silent lint',
68556 silly resolved        test: 'npm run --silent coverage',
68556 silly resolved        'tests-only': 'node test',
68556 silly resolved        readme: 'evalmd README.md',
68556 silly resolved        prelint: 'editorconfig-tools check * lib/* test/*',
