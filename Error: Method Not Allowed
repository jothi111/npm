71876 silly resolved     description: 'utilities for primitive JavaScript types',
71876 silly resolved     main: './lib/jsprim.js',
71876 silly resolved     repository: { type: 'git', url: 'git://github.com/joyent/node-jsprim.git' },
71876 silly resolved     dependencies:
71876 silly resolved      { 'assert-plus': '1.0.0',
