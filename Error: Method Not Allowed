72463 silly resolved     _shasum: '8c64ad5fd30dab1c976e2344ffe7f792a6a6df42',
72463 silly resolved     _from: 'require-directory@^2.1.1',
72463 silly resolved     _resolved: 'https://registry.npmjs.org/require-directory/-/require-directory-2.1.1.tgz' },
