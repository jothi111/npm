68556 silly resolved     keywords: [ 'querystring', 'qs' ],
68556 silly resolved     engines: { node: '>=0.6' },
68556 silly resolved     dependencies: {},
68556 silly resolved     devDependencies:
68556 silly resolved      { '@ljharb/eslint-config': '^12.2.1',
68556 silly resolved        browserify: '^16.2.0',
68556 silly resolved        covert: '^1.1.0',
68556 silly resolved        'editorconfig-tools': '^0.1.1',
