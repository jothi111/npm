73289 silly resolved        'browserify-istanbul': '^2.0.0',
73289 silly resolved        coveralls: '^2.11.9',
73289 silly resolved        eslint: '^2.9.0',
73289 silly resolved        istanbul: '^0.4.3',
73289 silly resolved        obake: '^0.1.2',
73289 silly resolved        'phantomjs-prebuilt': '^2.1.7',
73289 silly resolved        'pre-commit': '^1.1.3',
73289 silly resolved        reamde: '^1.1.0',
73289 silly resolved        rimraf: '^2.5.2',
