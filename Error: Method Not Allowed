68556 silly resolved   { name: 'qs',
68556 silly resolved     description: 'A querystring parser that supports nesting and arrays, with a depth limit',
68556 silly resolved     homepage: 'https://github.com/ljharb/qs',
68556 silly resolved     version: '6.5.2',
68556 silly resolved     repository: { type: 'git', url: 'https://github.com/ljharb/qs.git' },
